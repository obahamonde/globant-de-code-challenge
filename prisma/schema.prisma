generator client {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employees {
  id            Int         @id @default(autoincrement())
  name          String
  datetime      String
  department_id Int
  job_id        Int
  department    Departments @relation(fields: [department_id], references: [id])
  job           Jobs        @relation(fields: [job_id], references: [id])
}

model Departments {
  id         Int         @id @default(autoincrement())
  department String
  employees  Employees[]
}

model Jobs {
  id        Int         @id @default(autoincrement())
  job       String
  employees Employees[]
}
